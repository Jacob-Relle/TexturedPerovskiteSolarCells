    "from scipy.optimize import curve_fit\n",
    "def logifunc(x,A,x0,k,off):\n",
    "    return A / (1 + np.exp(-k*(x-x0))) + off\n",
    "\n",
    "def gauÃŸian(x,a,b,c):\n",
    "    return a*np.exp(- (x-b)**2/(2*c**2))\n",
    "\n",
